{
  "name": "cppshield",
  "displayName": "cppshield",
  "description": "cpp code helper",
  "version": "0.0.2",
  "publisher": "KalashShah",
  "repository": "https://github.com/microsoft/vscode-extension-samples/helloworld-sample",
  "engines": {
    "vscode": "^1.51.0"
  },
  "categories": ["Other"],
  "icon": "images/logo.png",
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cppshield.analyzeCode",
        "title": "Analyze C++ Code"
      }
    ],
    "activationEvents": [
      "onLanguage:cpp",
      "onCommand:cppshield.analyzeCode"
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },

  "devDependencies": {
    "@types/node": "^8.10.25",
    "@types/vscode": "^1.51.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  }
}
