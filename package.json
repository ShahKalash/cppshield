{
  "name": "cppshield",
  "displayName": "cppshield",
  "description": "cpp code helper",
  "version": "0.0.5",
  "publisher": "KalashShah",
  "repository": "https://github.com/ShahKalash/cppshield",
  "engines": {
    "vscode": "^1.51.0"
  },
  "categories": ["Other"],
  "icon": "images/logo.png",
  "main": "./out/extension.js",
  "activationEvents": [
    "onLanguage:cpp",
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "cppshield.analyzeCode",
        "title": "Cpp-Shield"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },

  "devDependencies": {
    "@types/node": "^8.10.25",
    "@types/vscode": "^1.51.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  }
}
